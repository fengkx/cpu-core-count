{
  "name": "cpu-core-count",
  "version": "0.1.0",
  "description": "Get Cpu Core count in or outside docker container",
  "files": [
    "dist",
    "tsdoc-metadata.json"
  ],
  "exports": {
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs"
    },
    "./bun": {
      "types": "./dist/bun.d.ts",
      "import": "./dist/bun.mjs",
      "require": "./dist/bun.cjs"
    },
    "./deno": {
      "types": "./dist/deno.d.ts",
      "import": "./dist/deno.mjs",
      "require": "./dist/deno.cjs"
    }
  },
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "doc": "api-extractor run --local && api-documenter markdown --input-folder ./temp/ --output-folder ./docs",
    "postdoc": "npm run format",
    "build": "bunchee && npm run doc",
    "format": "dprint fmt **/*.{ts,md,json,dockerfile}",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "sideEffects": "false",
  "author": "fengkx",
  "keywords": [
    "docker",
    "container",
    "cpu"
  ],
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.25.10",
    "@microsoft/api-extractor": "^7.47.5",
    "@types/bun": "^1.1.6",
    "@types/node": "^22.1.0",
    "bunchee": "^5.3.1",
    "dprint": "^0.47.2",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx dprint fmt --staged"
  }
}
